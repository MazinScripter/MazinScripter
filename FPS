local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Uni Scripts -"..game.Name,
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Uni's FPS changer",
   LoadingSubtitle = "by Unknowns",
   ShowText = "Uni Scripts", -- for mobile users to unhide rayfield, change if you'd like
   Theme = "Ocean", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   ToggleUIKeybind = "K", -- The keybind to toggle the UI visibility (string like "K" or Enum.KeyCode)

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "UniHub"
   },


   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Uni Scripts",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "1I1KBHE934Y", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"67KID"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})
local fps = 60
local Tab = Window:CreateTab("Changer", 4483362458) -- Title, Image
local Label = Tab:CreateLabel("FPS Changer ")
local Slider = Tab:CreateSlider({
   Name = "FPS",
   Range = {0, 500},
   Increment = 1,
   Suffix = "FPS",
   CurrentValue = 60,
   Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   fps = Value
   end,
})

local Input = Tab:CreateInput({
   Name = "Change it here :)",
   CurrentValue = "",
   PlaceholderText = "Type Your Fps Here",
   RemoveTextAfterFocusLost = false,
   Flag = "Input1",
   Callback = function(Text)
   fps = Text
   end,
})


local Button = Tab:CreateButton({
   Name = "Set FPS",
   Callback = function()
setfpscap(fps)
   end,
})
